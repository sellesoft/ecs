$ require "common"

@lpp.import "math/basic.lh"

#include <cmath>

namespace math
{

f32 floor(f32 x) { return ::floorf(x); }
f32 ceil(f32  x) { return ::ceilf(x);  }
f32 round(f32 x) { return ::roundf(x); }
f32 sqrt(f32  x) { return ::sqrtf(x);  }
f32 sin(f32   x) { return ::sinf(x);   } 
f32 cos(f32   x) { return ::cosf(x);   } 

}
